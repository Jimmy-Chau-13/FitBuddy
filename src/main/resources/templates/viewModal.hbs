<div class="modal fade" id="viewModal" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">View Settings</h4>
            </div>
            <div class="modal-body">
                <div>
                    <h4 id="theDateToShow" style="display: inline-block;"></h4>
                    <button type="button" class="btn btn-primary" id="addBtnFromView" style="float:right;" >Add Workout</button>
                </div>
                <div id="workoutTableDiv">

                </div>
                <!-- <div id="supersetTableDiv"></div> -->
                <p id="viewModalLog" style="color:red;"></p>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>

<script type="text/javascript">

    ////////////////////////////////////////// VIEW MODAL FUNCTIONALITY //////////////////////////////////////////////
    $('#viewModal').on('hidden.bs.modal', function() {
        $("#workoutTableDiv").empty();
        //$("#supersetTableDiv").empty();
        $("#viewModalLog").html("");
    });

    $('#addBtnFromView').on('click',function () {
        $("#viewModal").modal('hide');
        var editDate = $("#theDateToShow").text();
        $("#date").datepicker('setDate', editDate);
        $("#addModal").modal('show');
    });


    /////////////////////////////////////////////////////////////////////////////////////////////////////////////////
</script>
